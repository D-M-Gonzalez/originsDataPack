{
    "name": "Ojo de Águila",
    "description": "Tus flechas son rápidas y precisas",
    "type": "origins:multiple",
    "AttachTag": {
        "name": "Tag Applicator",
        "description": "Applies the Tag to arrows which allows the command to seek them out.",
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "/tag @e[type=arrow,distance=..2] add qaf"
        }
    },
    "Quick Flight": {
        "name": "Flight Script",
        "description": "Quicker arrows",
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "effect @e[tag=qaf] 2 2"
        }
    }
}